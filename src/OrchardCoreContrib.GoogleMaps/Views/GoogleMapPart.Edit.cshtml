@using OrchardCoreContrib.GoogleMaps.ViewModels
@model GoogleMapPartViewModel

<div class="form-group">
    <label asp-for="Latitude">@T["Latitude"]</label>
    <input asp-for="Latitude" class="form-control" />
    <span asp-validation-for="Latitude"></span>
</div>

<div class="form-group">
    <label asp-for="Longitude">@T["Longitude"]</label>
    <input asp-for="Longitude" class="form-control" />
    <span asp-validation-for="Longitude"></span>
</div>
